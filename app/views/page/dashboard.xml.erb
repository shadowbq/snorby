<?xml version="1.0"?>
<snorby dashboard_xml="1.0">
<time>
	<now><%=@now%></now>
	<cache><%=@last_cache%></cache>
</time>
<range><%=@range%></range>
<metrics>
	<src><%=@src_metrics%></src>
	<dst><%=@src_metrics%></dst>
	<tcp><%=@tcp%></tcp>
	<udp><%=@udp%></udp>
	<icmp><%=@icmp%></icmp>
</metrics>
<severities>
	<high><%=@high%></high>
	<medium><%=@medium%></medium>
	<low><%=@low%></low>
</severities>
<sensors>
	<metrics>
	<%=@sensor_metrics%>
	</metrics>
	<counts><%=@sensors.to_xml(:skip_instruct => true)%></counts>
</sensors>
<signatures><%=@signature_metrics%></signatures>
<events><%=@event_count%></events>
<classifications><%=@classifications.to_xml(:skip_instruct => true)%></classifications>
</snorby>
